import java.util.*;
class Menu
{
  public static void main (String[]args)
  {
    Scanner sc = new Scanner (System.in);
      LinkedList < Integer > x = new LinkedList < Integer > ();
    int n;
      System.out.
      println
      ("1.BeginInsert\n2.EndInsert\n3.Position Insert\n4.BeginDelete\n5.EndDelete\n6.Position Delete\n7.Display\n8.Exit\n-------------Enter ypur choice");
    int ch = sc.nextInt ();
    do
      {
	switch (ch)
	  {
	  case 1:
	    System.out.println ("Enter n value:");
	    n = sc.nextInt ();
	    x.addFirst (n);
	    break;
	    case 2:System.out.println ("Enter n value:");
	    n = sc.nextInt ();
	    x.addLast (n);
	    break;
	    case 3:System.out.println ("Enter n value:");
	    n = sc.nextInt ();
	    x.add (n);
	    break;
	    case 4:System.out.println ("Enter n value:");
	    n = sc.nextInt ();
	    x.removeFirst ();
	    break;
	    case 5:System.out.println ("Enter n value:");
	    n = sc.nextInt ();
	    x.removeLast ();
	    break;
	    case 6:System.out.println ("Enter n value:");
	    n = sc.nextInt ();
	    x.remove (n);
	    break;
	    case 7:System.out.println (x);
	  }
	System.out.
	  println
	  ("1.BeginInsert\n2.EndInsert\n3.PositionInsert\n4.BeginDelete\n5.EndDelete\n6.PositionDelete\n7.Display\n8.Exit\n--------------Enter your Choice:");
	ch = sc.nextInt ();
      }
    while (ch < 8);
  }
}
